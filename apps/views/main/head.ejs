<script>
    const socket = io('http://localhost:3000', { transports: ['websocket', 'polling', 'flashsocket'] });
    var peer = new Peer();
    var idUser = '<%=data.User.ID%>';
  
    peer.on('open', id => {
      socket.emit('NGUOI_DUNG_DANG_NHAP', { id: idUser, peerID: id });
    });
  </script>